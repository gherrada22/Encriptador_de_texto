#include<iostream>

using namespace std;

int main()
{
    int i,n,prime;
    cout<<"enter tne number n"<<endl;
    cin>>n;
    for(i=2;i<n;i++)
    {
        if(n%i==0)
        {
            prime=0;
            break;
        }
    }
    if(prime==0)
    {
        cout<<"it is not prime number"<<endl;
    }
    else
    {
        cout<<"it is prime number"<<endl;
    }
    return 0;
}
